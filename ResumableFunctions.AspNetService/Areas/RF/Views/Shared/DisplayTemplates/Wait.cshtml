@using ResumableFunctions.Handler.InOuts.Entities;
@model WaitEntity

@if (Model is MethodWaitEntity mw)
    Html.DisplayFor(x => mw, nameof(MethodWaitEntity));
else if (Model is WaitsGroupEntity group)
    Html.DisplayFor(x => group, nameof(WaitsGroupEntity));
else if (Model is FunctionWaitEntity function)
    Html.DisplayFor(x => function, nameof(FunctionWaitEntity));