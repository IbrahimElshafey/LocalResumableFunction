<div class="w3-container w3-margin">
    <header class="w3-text-blue-gray">
        <h2 class="w3-margin-top"><b>Pushed Call <span class="w3-text-indigo">ProjectSumbitted</span></b></h2>
    </header>


    <div class="w3-row-padding">
        <div class="w3-col s6">
            <header class="w3-text-indigo">
                <h4 class="w3-margin-top"><b>Input</b></h4>
            </header>
            <span class="JsonObject" style=" word-wrap: break-word;">
                {"CurrentProject":{"Id":100,"Name":"string","Description":"string","IsResubmit":false},"ManagerOneApproval":true,"ManagerTwoApproval":false,"ManagerThreeApproval":false,"ManagerFourApproval":false,"ManagerFiveApproval":false,"ExternalMethodStatus":"Not matched yet."}
            </span>
        </div>
        <div class="class=" w3-col s6"">
            <header class="w3-text-indigo">
                <h4 class="w3-margin-top"><b>Output</b></h4>
            </header>
            <span class="JsonObject" style=" word-wrap: break-word;">
                {"CurrentProject":{"Id":100,"Name":"string","Description":"string","IsResubmit":false},"ManagerOneApproval":true,"ManagerTwoApproval":false,"ManagerThreeApproval":false,"ManagerFourApproval":false,"ManagerFiveApproval":false,"ExternalMethodStatus":"Not matched yet."}
            </span>
        </div>
    </div>

